# 서버 설정
SERVER_URL=https://your-server.com
SOCKET_PORT=3000

# API 키 (서버 인증용)
API_KEY=your_api_key_here
API_SECRET=your_api_secret_here

# 암호화 키 (메시지 암호화용)
ENCRYPTION_KEY=your_encryption_key_32chars

# 사용자 정보 (초기 설정용)
DEFAULT_USER_ID=
DEFAULT_USER_TOKEN=

# 앱 설정
APP_ENV=development
DEBUG_MODE=false

# 푸시 알림 설정
PUSH_ENABLED=true
PUSH_SOUND=true

# 자동 업데이트 서버
UPDATE_SERVER_URL=https://your-update-server.com
